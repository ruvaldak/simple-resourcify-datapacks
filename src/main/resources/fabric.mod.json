{
	"schemaVersion": 1,
	"id": "simple-resourcify-datapacks",
	"version": "${version}",
	"name": "Simple Resourcify Datapacks",
	"description": "Configurable download location for Resourcify datapacks",
	"authors": [
		"AshlynnMods"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/simple-resourcify-datapacks/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "com.ashlynnmods.simpleresourcifydatapacks.SimpleResourcifyDatapacks",
				"adapter": "kotlin"
			}
		],
		"modmenu": [
			{
				"adapter": "kotlin",
				"value": "com.ashlynnmods.simpleresourcifydatapacks.integration.ModMenuIntegration"
			}
		]
	},
	"mixins": [
		"simple-resourcify-datapacks.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.18.1",
		"minecraft": ">=1.21.11",
		"java": ">=21",
		"fabric-api": "*",
		"fabric-language-kotlin": "*",
		"cloth-config": "*",
        "resourcify": "*"
	}
}